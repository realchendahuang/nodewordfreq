{
  "name": "nodewordfreq",
  "version": "0.1.1",
  "description": "TypeScript/Node.js 版 wordfreq，提供跨语言的词频查询能力。",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "data",
    "LICENSE.txt",
    "NOTICE.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "prepublishOnly": "npm run lint && npm run typecheck && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realchendahuang/nodewordfreq.git"
  },
  "keywords": [
    "word-frequency",
    "nlp",
    "tokenization",
    "zipf",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/realchendahuang/nodewordfreq/issues"
  },
  "homepage": "https://github.com/realchendahuang/nodewordfreq#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "lru-cache": "^11.2.2",
    "msgpackr": "^1.11.5",
    "nodejieba": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.14.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
